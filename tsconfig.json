{
  "compilerOptions": {
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@components": [
        "./src/components"
      ],
      "@constants/*": [
        "./src/constants/*"
      ],
      "@constants": [
        "./src/constants"
      ],
      "@interfaces/*": [
        "./interfaces/*"
      ],
      "@interfaces": [
        "./interfaces"
      ],
      "@server/*": [
        "./server/*"
      ],
      "@server": [
        "./server"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noEmit": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ]
}
